modelgee <- gee(TIMESHARE ~ PUBINJ + MOSHOME + GENDER + AGE + RACELG, id, data=dfslong,family=poisson, corstr = "independence")
modelgee <- gee(TIMESHARE ~ PUBINJ + MOSHOME + GENDER + AGE + RACELG, SUBJECT, data=dfslong,family=poisson, corstr = "independence")
?GEE
?gee
